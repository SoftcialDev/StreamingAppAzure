{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "common.schema.json",
  "title": "Common",
  "type": "object",
  "definitions": {
    "Empty": {
      "type": "object",
      "description": "An empty object for operations that don't require parameters."
    },
    "Timestamp": {
      "type": "object",
      "properties": {
        "ts": {
          "type": "string",
          "format": "date-time",
          "description": "ISO 8601 formatted timestamp."
        }
      },
      "required": ["ts"]
    }
  }
}
