{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "analysis-result.schema.json",
  "title": "AnalysisResult",
  "type": "object",
  "properties": {
    "user": {
      "type": "string",
      "description": "Email of the user associated with the frame."
    },
    "confidence": {
      "type": "number",
      "description": "Confidence level of the analysis."
    },
    "label": {
      "type": "string",
      "description": "Label assigned by the analysis."
    },
    "timestamp": {
      "type": "integer",
      "description": "Timestamp of the frame in milliseconds since epoch."
    }
  },
  "required": ["user", "confidence", "label", "timestamp"]
}
